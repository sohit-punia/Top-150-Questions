145. Binary Tree Postorder Traversal

class Solution {
    void postOrder(TreeNode* root, vector<int>& v){
        if(root == NULL) return;
        postOrder(root->left , v);
        postOrder(root->right , v);
        v.push_back(root->val);
    }
public:
    vector<int> postorderTraversal(TreeNode* root) {
        vector<int> v;
        postOrder(root , v);
        return v;
    }
};
