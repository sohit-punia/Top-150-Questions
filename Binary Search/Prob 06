153. Find Minimum in Rotated Sorted Array

    int findMin(vector<int>& nums) {
        int l = 0;
        int r = nums.size() - 1;
        while(l < r){
            int mid = l + (r - l)/2;
                if(nums[mid] > nums[r]) {
                // Minimum is in right half
                l = mid + 1;
            } 
            else {
                // Minimum is in left half (including mid)
                r = mid;
            }
            
        }
        return nums[l];
    }
