
9. Palindrome Number

    bool isPalindrome(int x) {
        if(x < 0) return false;
        int num = x;
        long sum = 0;
        while(x > 0){
            int rem = x % 10;
            x = x /10;
            sum = sum*10 + rem;
        }
        if(sum == num) return true;
        else return false;
    }
