45. Jump Game II


    int jump(vector<int>& nums) {
        int jump = 0;
        int curr = 0; 
        int farthest = 0;
        for(int i =0; i< nums.size() - 1; i++){
            farthest = std::max( farthest , nums[i] + i);
            if( i == curr){
                curr = farthest;
                jump++;
            }
        }
        return jump;
    }
