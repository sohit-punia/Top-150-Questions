26. Remove Duplicates from Sorted Array


    int removeDuplicates(vector<int>& nums) {
        if(nums.size() == 0) return 0;
        int j  =0 ;
        for(int i  = 0; i< nums.size(); i++){
            if(nums[j] != nums[i]){
                j++;
                nums[j] = nums[i];
            }
        }
        return j +1;
    }
