242. Valid Anagram


    bool isAnagram(string s, string t) {
        if(t.length() < s.length()) return false;
        unordered_map <char , int> mp;
        for(char c : s){
            mp[c]++;
        }
        for(char c : t){
            if(mp[c] == 0) return false;
            mp[c]--;
        }
        return true;
    }
